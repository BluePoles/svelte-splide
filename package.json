{
  "name": "@splidejs/svelte-splide",
  "version": "0.1.0",
  "description": "Svelte component for the Splide slider/carousel.",
  "keywords": [
    "splide",
    "slider",
    "carousel",
    "slideshow",
    "lightweight",
    "touch",
    "responsive",
    "svelte"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Splidejs/svelte-splide.git"
  },
  "bugs": {
    "url": "https://github.com/Splidejs/svelte-splide/issues"
  },
  "scripts": {
