    "eslint": "^7.32.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "gh-pages": "^3.2.3",
    "jest": "^27.3.1",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.9.8",
    "svelte2tsx": "^0.4.7",
    "tslib": "^2.3.1",
